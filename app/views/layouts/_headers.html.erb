<% if logged_in? %>
    <%= link_to current_user.username, user_path(id: session[:user_id]) %>
    <%= link_to 'Logout', logout_path, method: :delete %>
  <% else %>
    <%= link_to 'Login', login_path %>
    <%= link_to 'Create an account', signup_path %>
<% end %>
  <%= link_to 'home', root_path %>